                        span Вес 1285 г.
                    li(data-target="2").menu-controls-item
                        span 1800 р. #[br]
                        span Вес 1285 г.
                    li(data-target="3").menu-controls-item.menu-controls-item--active
                        span 2000 р. #[br]
                        span Вес 1285 г.
                    li(data-target="4").menu-controls-item
                        span 2500 р. #[br]
                        span Вес 1285 г.
                    li(data-target="5").menu-controls-item
                        span 2750 р. #[br]
                        span Вес 1285 г.
                    li(data-target="6").menu-controls-item
                        span 3000 р. #[br]
                        span Вес 1285 г.
                    li(data-target="7").menu-controls-item
                        span 3500 р. #[br]
                        span Вес 1285 г.
                    li(data-target="8").menu-controls-item
                        span 3880 р. #[br]
                        span Вес 1285 г.
                    li(data-target="9").menu-controls-active-marker
